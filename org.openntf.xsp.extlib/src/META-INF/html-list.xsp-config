<faces-config>
	<faces-config-extension>
		<namespace-uri>http://openntf.org/p/Community%20Control%20Library</namespace-uri>
		<default-prefix>openntf</default-prefix>
	</faces-config-extension>
	<component id="HtmlList">
		<description>This is an HTML list ("UL")</description>
		<display-name>HTML List</display-name>
		<component-type>org.openntf.xsp.extlib.components.HtmlList</component-type>
		<component-class>org.openntf.xsp.extlib.components.HtmlList</component-class>
		<group-type-ref>org.openntf.xsp.group.html.common</group-type-ref>
		<component-extension>
			<component-family>org.openntf.xsp.extlib</component-family>
			<tag-name>htmlList</tag-name>
			<renderer-type>org.openntf.xsp.extlib.HtmlList</renderer-type>
			<designer-extension>
				<in-palette>true</in-palette>
				<category>HTML Tags</category>
			</designer-extension>
		</component-extension>
		<property id="ordered">
			<description>Specify if this is an ordered list. Defaults to false.</description>
			<display-name>Ordered List</display-name>
            <property-name>ordered</property-name>
			<property-class>java.lang.Boolean</property-class>
		</property>
	</component>

	<component id="HtmlListItem">
		<description>This is an HTML list item ("LI")</description>
		<display-name>HTML List Item</display-name>
		<component-type>org.openntf.xsp.extlib.components.HtmlListItem</component-type>
		<component-class>org.openntf.xsp.extlib.components.HtmlListItem</component-class>
		<group-type-ref>org.openntf.xsp.group.html.common</group-type-ref>
		<component-extension>
			<component-family>org.openntf.xsp.extlib</component-family>
			<tag-name>htmlListItem</tag-name>
			<renderer-type>org.openntf.xsp.extlib.HtmlListItem</renderer-type>
			<designer-extension>
				<in-palette>true</in-palette>
				<category>HTML Tags</category>
			</designer-extension>
		</component-extension>
	</component>
</faces-config>