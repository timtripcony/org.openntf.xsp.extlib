<faces-config>
	<faces-config-extension>
		<namespace-uri>http://openntf.org/p/Community%20Control%20Library</namespace-uri>
		<default-prefix>openntf</default-prefix>
	</faces-config-extension>
	<component>
		<description>A control that allows for dynamic, scriptable rendering of 2D shapes and bitmap images</description>
		<display-name>Canvas</display-name>
		<component-type>org.openntf.xsp.Canvas</component-type>
		<component-class>org.openntf.xsp.extlib.components.XspCanvas</component-class>
		<group-type-ref>org.openntf.xsp.group.html.common</group-type-ref>
		<group-type-ref>org.openntf.xsp.group.events.touch</group-type-ref>
		<component-extension>
			<base-component-type>org.openntf.xsp.HtmlTag</base-component-type>
			<component-family>com.ibm.xsp.Tag</component-family>
			<renderer-type>org.openntf.xsp.Canvas</renderer-type>
			<tag-name>canvas</tag-name>
			<designer-extension>
				<generate-id>false</generate-id>
				<in-palette>true</in-palette>
			</designer-extension>
		</component-extension>
	</component>
</faces-config>